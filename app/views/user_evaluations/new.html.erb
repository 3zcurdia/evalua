<% content_for(:breadcrumbs) do %>
<li>
  <%= link_to 'Evaluations', evaluations_path %>
</li>
<li>
  <% if @evaluation.type == 'Evaluations::Repository' %>
    <%= link_to 'Repository', evaluations_repositories_path %>
  <% else %>
    <%= link_to 'Pull Request', evaluations_pull_requests_path %>
  <% end %>
</li>
<li>
  <%= link_to @evaluation.name, @evaluation %>
</li>
<li class="is-active">
  <%= link_to 'Submit', new_evaluation_user_evaluation_path(@evaluation) %>
</li>
<% end %>

<h1 class="title">Subtmit Evaluation</h1>
<hr>

<%= render 'form', user_evaluation: @user_evaluation %>
